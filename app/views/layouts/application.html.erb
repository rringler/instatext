<!DOCTYPE html>
<html>
  <head>
    <title>Instatext</title>
    <%= stylesheet_link_tag 'application', media: :all %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div id='wrap'>
      <%= render 'layouts/navbar' %>

      <div id='content' class='container'>
        <%= yield %>
      </div>

      <div id='push'></div>
    </div>

    <%= render 'layouts/footer' %>
    <%= render 'layouts/google_analytics' %>
  </body>
</html>